{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{if .Root}} ‚ö° {{end}}{{.UserName}} ‚ûî üìÅ{{.Folder}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .WSL }}WSL at{{ end }} {{.Icon}}  ",
          "foreground": "#ffffff",
          "background": "#272625",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " ‚öô ",
          "foreground": "#272625",
          "background": "#ff710a",
          "type": "text",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "max_depth": 1,
            "style": "folder",
            "trim_prefix": true,
            "bold": true
          },
          "template": " {{ .Folder }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#878c91",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue0a0 ",
            "branch_identical_icon": "\u25cf",
            "branch_max_length": 25,
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#272625",
          "powerline_symbol": "\ue0b0",
          "background": "#28f063",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffc642{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff007a{{ end }}",
            "{{ if gt .Ahead 0 }}#d342ff{{ end }}",
            "{{ if gt .Behind 0 }}#4788f5{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_default": false,
            "bold": true
          },
          "template": " \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "type": "aws",
          "style": "diamond",
          "background": "#666369",
          "background_templates": [
            "{{if contains \"default\" .Profile}}#f3d56e{{end}}",
            "{{if contains \"jan\" .Profile}}#ff007a{{end}}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf0ad ",
          "foreground": "#272625",
          "powerline_symbol": "\ue0b0",
          "background": "#ff710a",
          "type": "root",
          "style": "powerline"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "fetch_package_manager": true,
            "npm_icon": " <#ff007a>\ue5fa</> ",
            "yarn_icon": " <#4788f5>\ue6a7</>",
            "bold": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "foreground": "#84ff39",
          "background": "#f3d56e",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_virtual_env": true,
            "bold": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "#fee66b",
          "background": "#3774a6",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "bold": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#eb1f24",
          "background": "#0c89c6",
          "type": "java",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "bold": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#ffffff",
          "background": "#4788f5",
          "type": "go",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "bold": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\uf104<#f3d56e>\uf0e7</>\uf105 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#ffffff",
          "background": "#666369",
          "type": "azfunc",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "charged_icon": "\uf240 ",
            "charging_icon": "\uf1e6 ",
            "discharging_icon": "\ue234 ",
            "bold": true
          },
          "template": "{{ if and (ne .State.String \"Charging\") (ne .State.String \"Full\") }} {{ .Icon }}{{ .Percentage }}\uf295 {{ end }}",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b2",
          "background": "#ff007a",
          "type": "battery",
          "style": "diamond",
          "background_templates": [
            "{{if ge .Percentage 75.0}}#84ff39{{end}}",
            "{{if and (ge .Percentage 25.0) (lt .Percentage 75.0)}}#f3d56e{{end}}",
            "{{if lt .Percentage 25.0}}#ff007a{{end}}"
          ],
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "!",
          "foreground": "#ff007a",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "‚ùØ",
          "foreground": "#ff710a",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
