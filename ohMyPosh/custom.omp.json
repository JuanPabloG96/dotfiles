{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{if .Root}} ‚ö° {{end}}{{.UserName}} ‚ûî üìÅ{{.Folder}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .WSL }}WSL at{{ end }} {{.Icon}}  ",
          "foreground": "#c0caf5",
          "background": "#1a1b26",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " ‚öô ",
          "foreground": "#1a1b26",
          "background": "#7aa2f7",
          "type": "text",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "max_depth": 1,
            "style": "folder",
            "trim_prefix": true,
            "bold": true
          },
          "template": " {{ .Folder }} ",
          "foreground": "#c0caf5",
          "powerline_symbol": "\ue0b0",
          "background": "#565f89",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue0a0 ",
            "branch_identical_icon": "\u25cf",
            "branch_max_length": 25,
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#1a1b26",
          "powerline_symbol": "\ue0b0",
          "background": "#9ece6a",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#e0af68{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f7768e{{ end }}",
            "{{ if gt .Ahead 0 }}#bb9af7{{ end }}",
            "{{ if gt .Behind 0 }}#7aa2f7{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_default": false,
            "bold": true
          },
          "template": " \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
          "foreground": "#c0caf5",
          "powerline_symbol": "\ue0b0",
          "type": "aws",
          "style": "diamond",
          "background": "#414868",
          "background_templates": [
            "{{if contains \"default\" .Profile}}#e0af68{{end}}",
            "{{if contains \"jan\" .Profile}}#f7768e{{end}}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf0ad ",
          "foreground": "#1a1b26",
          "powerline_symbol": "\ue0b0",
          "background": "#7aa2f7",
          "type": "root",
          "style": "powerline"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "fetch_package_manager": true,
            "npm_icon": " <#f7768e>\ue5fa</> ",
            "yarn_icon": " <#7aa2f7>\ue6a7</>",
            "bold": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "foreground": "#9ece6a",
          "background": "#e0af68",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_virtual_env": true,
            "bold": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "#e0af68",
          "background": "#7aa2f7",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "bold": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#f7768e",
          "background": "#1a1b26",
          "type": "java",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "bold": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#c0caf5",
          "background": "#bb9af7",
          "type": "go",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "bold": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\uf104<#e0af68>\uf0e7</>\uf105 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#c0caf5",
          "background": "#414868",
          "type": "azfunc",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "!",
          "foreground": "#f7768e",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "‚ùØ",
          "foreground": "#7aa2f7",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
